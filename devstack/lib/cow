set +o xtrace
COW_DIR=$DEST/devstack-plugin-cow
COW_CONF_DIR=/etc/cow
COW_CONF=$COW_CONF_DIR/cow.conf
COW_BIN_DIR=$COW_DIR/bin

function install_cow() {
    echo "INSTALLING COW"

    git_clone https://github.com/misterzorgy2000/cow.git
}

function start_cow() {
    sleep 10
    echo "---------------------------STARTING COW------------------------------"
    run_process cow-engine "$COW_BIN_DIR/cow-engine --config-file $COW_CONF"
}